{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-param-reassign": 0,
        "import/prefer-default-export": 0,
        "no-inner-declarations": 0,
        "no-plusplus": 0,
        "no-use-before-define": 0,
        "import/extensions": 0,
        "import/no-cycle": 0
    }
}
